function toplam = CiftlerinKuplerininToplaminiHesapla( A )
%CİFTLERİNKUPLERİNİNTOPLAMİNİHESAPLA Summary of this function goes here
%   Detailed explanation goes here
toplam=0;
diziBoyutu=length(A);
for i=1:diziBoyutu
    if mod(A(i),2)==0 %dizi elemanı çift sayı ise
        toplam=toplam+A(i)^3;
    end
end

end
